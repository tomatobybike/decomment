    "typescript",
    "jsx",
    "tsx",
    "vue",
    "cli",
    "code-quality",
    "refactor",
    "eslint",
    "license"
  ],
  "author": "King Monkey",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tomatobybike/decomment.git"
  },
  "bugs": {
    "url": "https://github.com/tomatobybike/decomment/issues"
  },
  "homepage": "https://github.com/tomatobybike/decomment#readme",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@babel/parser": "^7.24.0",
    "@babel/traverse": "^7.24.0",
    "@babel/generator": "^7.24.0",
    "glob": "^10.3.10"
  },
  "license": "MIT",
  "packageManager": "yarn@1.22.22"
